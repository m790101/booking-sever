{
    "version": 2,
    "builds": [
     {
      "src": "src/app.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] },
      "installCommand": "npm install"
     }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "src/app.ts"
        }
      ]
   }