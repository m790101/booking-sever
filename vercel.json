{
    "version": 2,
    "installCommand": "npm install",
    "builds": [
     {
      "src": "src/app.ts",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
     }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "src/app.ts",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
          "headers": {
            "Access-Control-Allow-Origin": "*"
          }

        }
      ]
   }